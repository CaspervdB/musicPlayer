<Window x:Class="WpfAppMusicLibraryTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppMusicLibraryTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="130.517" Width="391.197">
    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>
    <Grid Margin="0,0,0,17">
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="363" Margin="10,38,0,0" Height="45" Command="{Binding ButtonCommand}"/>
        <TextBox HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding Link}" VerticalAlignment="Top" Width="363" Margin="10,10,0,0"/>
    </Grid>
</Window>
